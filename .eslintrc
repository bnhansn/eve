{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "plugins": [
    "react",
    "flowtype"
  ],
  "extends": ["airbnb"],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "15.3.1"
    }
  },
  "globals": {
    "document": true,
    "module": true,
    "require": true,
    "window": true,
    "dispatch": true,
    "fetch": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "react/no-unused-prop-types": 0
  }
}
